generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  username String @unique
}

model Problem {
  id          String @id @default(cuid())
  title       String @unique
  description String
  solution    String
}

model Contest {
  id   String @id @default(cuid())
  name String
}
